{
  "name": "jdbc",
  "version": "0.0.5",
  "description": "Node Module JDBC wrapper",
  "main": "index.js",
  "dependencies": {
    "java": "~0.3.1",
    "underscore": "~1.6.0",
    "promise": "~4.0.0"
  },
